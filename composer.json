{
    "name": "m10c/unlocked-analytics-bundle",
    "description": "Analytics server bundle for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "symfony/config": "^6.4 | ^7.0 | ^8.0",
        "symfony/dependency-injection": "^6.4 | ^7.0 | ^8.0",
        "symfony/http-kernel": "^6.4 | ^7.0 | ^8.0",
        "symfony/routing": "^6.4 | ^7.0 | ^8.0",
        "doctrine/orm": "^3.0",
        "doctrine/persistence": "^2.0 | ^3.0",
        "ramsey/uuid-doctrine": "^1.6 | ^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.75",
        "symfony/phpunit-bridge": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "M10c\\UnlockedAnalyticsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "M10c\\UnlockedAnalyticsBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs": "php vendor/bin/php-cs-fixer fix",
        "phpunit": "php vendor/bin/simple-phpunit",

        "test": [
            "@phpunit"
        ]
    }
}
